<h1>Houses#index</h1>
<table class="table table-bordered">
<thead>
  <tr class="info">
    <td>No</td>
    <td>Tipe</td>
    <td>Alamat</td>
    <td>Pilihan</td>
  </tr>
</thead>
<% @houses.each do |house| %>
  <tr class=warning>
    <td><%= house.houseno %></td>
    <td><%= house.tipe %></td>
    <td><%= house.address %></td>
    <td><%= link_to 'Edit', edit_house_path(house.id) %> | <%= link_to 'Delete', house_path(house.id), method: "DELETE", data:{confirm: 'Delete this data?'} %></td>
  </tr>
<% end %>
</table>
<%= link_to 'Create New', new_house_path %>
